@using EntityLayer.Concrete
@model List<Destination>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Destination Index</h1>


<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>City</th>
        <th>Price</th>
        <th>Capacity</th>
        <th>Details</th>
        <th>See</th>
        <th>Update</th>
        <th>Delete</th>
    </tr>
    @foreach(var item in Model ){

        <tr>
            <td>@item.DestinationID</td>
            <td>@item.City</td>
            <td>@item.Price</td>
            <td>@item.Capacity</td>
            <td>@item.Description</td>
            <td><a href="#" class="btn btn-outline-success">See</a></td>
            <td><a href="#" class="btn btn-outline-info">Update</a></td>
            <td><a href="#" class="btn btn-outline-danger">Delete</a></td>
        </tr>
    }
</table>